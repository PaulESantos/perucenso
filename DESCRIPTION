Package: perucenso
Title: Procesar Datos del Censo Poblacional del Perú 2017
Version: 0.0.0.1
Authors@R: 
    c(person(given = "Paul Efren",
           family = "Santos Andrade",
           role = c("aut", "cre"),
           email = "paulefrens@gmail.com",
           comment = c(ORCID = "0000-0002-6635-0375")),
